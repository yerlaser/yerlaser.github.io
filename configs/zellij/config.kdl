keybinds {
  pane {
    bind "Ctrl w" { SwitchFocus; }
  }
  shared_except "pane" "locked" {
    bind "Ctrl w" { SwitchToMode "Pane"; }
  }
  shared_except "locked" {
    bind "Alt n" { NewPane "Down"; }
    bind "Alt p" { NewPane "Right"; }
    bind "Ctrl q" { ToggleActiveSyncTab; SwitchToMode "Normal"; }
    bind "Alt b" { SwitchToMode "Resize"; }
    bind "Ctrl ^" { Quit; }
  }
}
