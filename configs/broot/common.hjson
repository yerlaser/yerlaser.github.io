show_selection_mark: true
quit_on_last_cancel: true
content_search_max_file_size: 10MB
verbs: [
    {
        key: "alt-f"
        external: 'sh -c "echo {file} | pbcopy"'
    }
    {
        key: "tab"
        internal: ":line_down"

    }
    {
        key: "backtab"
        internal: ":line_up"
    }
]
