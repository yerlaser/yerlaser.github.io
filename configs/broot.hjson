show_selection_mark: true

quit_on_last_cancel: true

content_search_max_file_size: 10MB

verbs: [
    {
        key: "enter"
        external: "hx -c /Users/yerlan/Published/configs/config.toml {file}:{line}"
        apply_to: file
    }
    {
        key: "alt-f"
        internal: ":print_relative_path"
    }
    {
        key: "tab"
        internal: ":line_down"

    }
    {
        key: "backtab"
        internal: ":line_up"
    }
]
